{
    "type": "index_parallel",
    "spec": {
        "ioConfig": {
            "type": "index_parallel",
            "inputSource": {
                "type": "s3",
                "prefixes": [
                    "s3://{{var.value.get("bucket_name")}}/auth/auth_event/"
                ],
                "endpointConfig": {
                    "url": "{{var.value.get("endpoint_url")}}",
                    "signingRegion": "kr-standard"
                },
                "properties": {
                    "accessKeyId": "{{var.value.get("obj_public_key")}}",
                    "secretAccessKey": "{{var.value.get("obj_secret_key")}}"
                }
            },
            "inputFormat": {
                "type": "parquet"
            }
        },

        "dataSchema": {
            "dataSource": "auth-{{ ds }}-{{ task_instance.try_number }}",
            "timestampSpec": {
                "column": "ts",
                "format": "millis"
            },
            "dimensionsSpec": {
                "dimensions": [
                    {
                        "name": "level",
                        "type": "string"
                    },
                    {
                        "name": "city",
                        "type": "string"
                    },
                    {
                        "name": "stateName",
                        "type": "string"
                    },
                    {
                        "name": "userAgent",
                        "type": "string"
                    },
                    {
                        "name": "gender",
                        "type": "string"
                    },
                    {
                        "name": "registration",
                        "type": "long"
                    },
                    {
                        "name": "success",
                        "type": "string"
                    }
                ]
            },
            "granualitySpec": {
                "type": "uniform",
                "intervals": [
                    "2023-07-08/2023-07-09"
                ],
                "rollup": false
            }
        }
    }
}
