{
    "type": "index_parallel",
    "spec": {
        "ioConfig": {
            "type": "index_parallel",
            "inputSource": {
                "type": "s3",
                "prefixes": [
                    "s3://{{var.value.get("bucket_name")}}/listen/listen_joined_event/date_id={{yesterday_ds}}"
                ],
                "endpointConfig": {
                    "url": "{{var.value.get("endpoint_url")}}",
                    "signingRegion": "kr-standard"
                },
                "properties": {
                    "accessKeyId": "{{var.value.get("obj_public_key")}}",
                    "secretAccessKey": "{{var.value.get("obj_secret_key")}}"
                }
            },
            "inputFormat": {
                "type": "parquet"
            },
            "appendToExisting" : true
        },
        "dataSchema": {
            "dataSource": "listen",
            "timestampSpec": {
                "column": "ts",
                "format": "millis"
            },
            "dimensionsSpec": {
                "dimensions": [
                    {
                        "name": "artist_name",
                        "type": "string"
                    },
                    {
                        "name": "title",
                        "type": "string"
                    },
                    {
                        "name": "auth",
                        "type": "string"
                    },
                    {
                        "name": "level",
                        "type": "string"
                    },
                    {
                        "name": "city",
                        "type": "string"
                    },
                    {
                        "name": "state",
                        "type": "string"
                    },
                    {
                        "name": "stateName",
                        "type": "string"
                    },
                    {
                        "name": "gender",
                        "type": "string"
                    },
                    {
                        "name": "song_id",
                        "type": "string"
                    },
                    {
                        "name": "year",
                        "type": "float"
                    }
                ]
            },
            "metricsSpec": [
                {
                    "type": "doubleAny",
                    "name": "duration",
                    "fieldname": "duration"
                },
                {
                    "type" : "stringAny",
                    "name" : "userAgent",
                    "fieldName" : "userAgent"
                },
                {
                    "type" : "longAny",
                    "name" : "registration",
                    "fieldName" : "registration"
                },
                {
                    "type": "floatAny",
                    "name": "key",
                    "fieldName" : "key"
                },
                {
                    "type": "doubleAny",
                    "name": "key_confidence",
                    "fieldName": "key_confidence"
                },
                {
                    "type": "doubleAny",
                    "name": "loudness",
                    "fieldName": "loudness"
                },
                {
                    "type": "doubleAny",
                    "name": "song_hotness",
                    "fieldName": "song_hotttnesss"
                },
                {
                    "type": "doubleAny",
                    "name": "tempo",
                    "fieldName": "tempo"
                }
            ]
        }
    }
}
